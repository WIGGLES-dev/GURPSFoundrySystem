{
    "compilerOptions": {
        "skipLibCheck": true,
        "importsNotUsedAsValues": "preserve",
        "esModuleInterop": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "baseUrl": "./src",
        "paths": {},
        "lib": [
            "webworker",
            "scripthost",
            "ESNext",
            "DOM"
        ],
        "types": [
            "node",
            "foundry-pc-types",
            "svelte",
            "copy-webpack-plugin",
        ],
        "target": "ES2015"
    }
}